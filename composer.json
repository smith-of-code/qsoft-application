{
  "name": "qsoft/new-project",
  "description": "new project",
  "license": "proprietary",
  "authors": [
    {
      "name": "QSoft Team",
      "email": "support@qsoft.ru"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "./libs/qsoft-application"
    },
    {
      "type": "path",
      "url": "./libs/qsoft_database"
    },
    {
      "type": "path",
      "url": "./libs/qsoft-foundation"
    },
    {
      "type": "path",
      "url": "./libs/qsoft_migration"
    },
    {
      "type": "path",
      "url": "./libs/qsoft-queue"
    }
  ],
  "require": {
    "php": "~8.1",
    "qsoft/foundation": "dev-main",
    "qsoft/migration": "dev-main",
    "qsoft/application": "dev-main",
    "qsoft/database": "dev-main",
    "qsoft/queue": "dev-main",
    "guzzlehttp/guzzle": "^7.5",
    "ext-json": "*",
    "hflabs/dadata": "^22.3",
    "psr/http-message": "^1.0",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "QSoft\\": ["app/core/local/src"],
      "QSoft\\Commands\\": ["app/commands"]
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    }
  }
}
